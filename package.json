{
  "name": "approachable-backend",
  "version": "1.0.0",
  "description": "Project for Brigham Andersen and Taylor English",
  "main": "server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon src/server.ts",
    "db-init": "npx prisma migrate dev --name init",
