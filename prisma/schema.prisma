generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db.sqlite3"
}

model User {
  id                   Int       @id @default(autoincrement())
  firstName            String
  lastName             String
  birthDate            Int
  bio                  String?
  latitude             Float
  longitude            Float
  locationLastUpdated  Int
}